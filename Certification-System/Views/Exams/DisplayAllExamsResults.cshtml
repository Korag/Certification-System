@model IEnumerable<Certification_System.DTOViewModels.DisplayExamResultViewModel>
@using Certification_System.DTOViewModels

@{
    ViewData["Title"] = "DisplayAllExamsResults";
    Layout = "~/Views/Shared/_Layout.cshtml";
};

<h1>Lista wyników z egzaminów</h1>
<hr />

@if (!string.IsNullOrWhiteSpace(ViewBag.message))
{
    <h2 class="alert alert-info" role="alert">@ViewBag.message</h2>
}

@await Component.InvokeAsync("DisplayExamResultDataTable", new { examResultViewModel = Model, tableIdentificator = "listExamsResults", operationSet = 1 })

@section Scripts{
    @{await Html.RenderPartialAsync("_DataTableBasicScript", new DataTableInitializationViewModel("listExamsResults", new int[] { }, 80, true));}
}
