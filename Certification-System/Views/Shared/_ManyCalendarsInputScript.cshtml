@model Certification_System.DTOViewModels.ManyCalendarsInputScriptViewModel

<script>
        $(function () {
             for (var i = 0; i < @Model.QuantityOfInputs; i++) {
                $("#@Model.Date" + i).datetimepicker({
                    format: 'DD.MM.YYYY HH:mm',
                    locale: moment.locale('pl', {
                        week: { dow: 1 }
                    })
                });
            }
        });
</script>